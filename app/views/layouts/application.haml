!!! XML
%html{:dir => "ltr", :lang => "en-US"}
  %head
    %meta{:charset => "UTF-8"}
    %title Titanium Folio
    = stylesheet_link_tag 'http://gmpg.org/xfn/11', :media => 'profile'
    = stylesheet_link_tag 'global.css', :rel => 'stylesheet', :type => 'text/css'
    = stylesheet_link_tag 'theme.css', :rel => 'stylesheet', :type => 'text/css'
    = stylesheet_link_tag 'script/fancybox/jquery.fancybox-1.3.1.css', :rel => 'stylesheet', :type => 'text/css'
    = stylesheet_link_tag 'favicon.ico', :rel => 'shortcut icon', :type => 'image/vnd.microsoft.icon'
    = stylesheet_link_tag 'favicon.ico', :rel => 'icon', :type => 'image/vnd.microsoft.icon'
    = javascript_include_tag 'jquery-1.4.2.min.js'
    = javascript_include_tag 'cufon-yui.js'
    = javascript_include_tag 'Bebas_Neue_400.font.js'
    = javascript_include_tag 'jquery.quicksand.min.js'
    = javascript_include_tag 'jquery.easing.1.3.js'
    = javascript_include_tag 'jquery-css-transform.js'
    = javascript_include_tag 'jquery-animate-css-rotate-scale.js'
    = javascript_include_tag 'fancybox/jquery.fancybox-1.3.1.js'
    = javascript_include_tag 'loopedslider.js'
    = javascript_include_tag 'myJS.js'
  %script{:type => "text/javascript"}
    :plain
      $(document).ready(function(){
        $('.search_input').focus();
        $(window).mouseover(function(){
          $('.no_show').fadeIn(1000);
        });
      });
  %body
    #header
      .header-content
        .logo
        / Main menu
        #access{:role => "navigation"}
          .menu
            %ul
              %li.current_page_item
                %a.bebas-font{:href => "index.html"} Super Street Fighter 4
              %li.page_item.page-item-2
                %a.bebas-font{:href => "about.html"} Feedback
    #wrapper
      / Page header
      .page_header
        %h1.bebas-font 
          Match-up
        %h2.bebas-font 
          Giant penis
        .clearfloat
      / Breadcrumb
      .breadcrumb
        %ul
          %li
            %a{:href => "index.html"}> Ryu
            \&nbsp;&nbsp;/&nbsp;&nbsp;
          %li vs Akuma
        .clearfloat
      / Page content
      .page_content
        / Sidebar left
        .sidebar_left
          %h2.bebas-font Characters
          %ul.categories
            %li
              %a{:href => ""} Ryu
            %li
              %a{:href => ""} Abel
            %li
              %a{:href => ""} Ryu
            %li
              %a{:href => ""} Abel
            %li
              %a{:href => ""} Ryu
            %li
              %a{:href => ""} Abel
            %li
              %a{:href => ""} Ryu
            %li
              %a{:href => ""} Abel
            %li
              %a{:href => ""} Ryu
            %li
              %a{:href => ""} Abel
          .clearfloat
        / Content right
        .content_right
          = yield 
