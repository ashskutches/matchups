%table
  %tr
    %td
      %a{:href => "/"} TauntWinFree.com
    - if user_signed_in?
      %td#user
        = current_user.email
        %a{:href => "/users/sign_out"} / Sign out
    - else
      %td#user
        %a{:href => "/users/registration/sign_up"} Sign up /
        %a{:href => "/users/sign_in"} Log In
