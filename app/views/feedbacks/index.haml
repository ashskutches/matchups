- if current_user && current_user.admin?
  %h2 Matchup: Feedback List
  %hr
  #feedback
    %ul
      - feedbacks.each do |feedback|
        %li
          %h3
            %b= feedback.referer
            = button_to "Remove", feedback_path(feedback), :method => :delete, :style => "float: right;"
            (
            = feedback.created_at.to_date
            )
          %p
            = feedback.body
- else
  %h2 Your not an admin!
