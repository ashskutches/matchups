- unless current_user && current_user.admin
  %h2 Matchup: Feedback List
  %hr
  #feedback
    %ul
      - feedbacks.each do |feedback|
        %li
          %h3
            %b= feedback.referer
            (
            = feedback.created_at.to_date
            )
          %p
            = feedback.body
- else
  %h3
    Hey your not an Admin!
