- if current_user && current_user.admin
  #feedback
    %h2 Matchup: Feedback List
    %hr
    - @feedbacks.each do |feedback|
      %br
      %h2
        = feedback.kind.titleize
      %p
        = feedback.body
      %p
        *
        %b= feedback.referer
        (
        = feedback.created_at.to_date
        )
        %hr
- else
  %h3
    Hey your not an Admin!
